db.system.js.save({
  _id: "findpassword",
  value: function (urlsearch){
    return db.thePasswordsCollection.find( { URL:urlsearch } );
  }	
});

db.system.js.save({
  _id: "findhttps",
  value: function (){
    return db.thePasswordsCollection.find( {URL: /.*https.*/} );
  }	
});

db.system.js.save({
  _id: "changeurl",
  value: function (passwordfind, newurl){
    return db.thePasswordsCollection.update({password: passwordfind},{$set: {URL: newurl}});
  }	
});
db.system.js.save({
  _id: "changepass",
  value: function (newpassword, urlfind){
    return db.thePasswordsCollection.update({URL: urlfind},{$set: {password: newpassword}});
  }	
});

db.system.js.save({
  _id: "removepass",
  value: function (passwordfind){
    return db.thePasswordsCollection.remove( {password: passwordfind} );
  }	
});
